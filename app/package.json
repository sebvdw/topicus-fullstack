    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~5.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
