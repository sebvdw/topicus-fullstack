{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "ng lint",
    "format": "prettier --write .",
    "gen:api": "openapi-generator-cli generate -i openapi.yaml -g typescript-angular -o src/app/core/api/v1",
    "gen:api:local": "rm -rf src/app/core/api/v1 && openapi-generator-cli generate -i http://localhost:5251/swagger/v1/swagger.yaml -g typescript-angular -o src/app/core/api/v1"
  },
  "private": true,
