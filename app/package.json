    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@ngrx/schematics": "^17.2.0",
    "@types/jasmine": "~5.1.0",
